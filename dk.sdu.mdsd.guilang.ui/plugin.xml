			class="dk.sdu.mdsd.guilang.ui.GuilangExecutableExtensionFactory:org.eclipse.xtext.resource.IResourceFactory"
			type="template">
		</parser>
	</extension>
	<extension point="org.eclipse.xtext.extension_resourceServiceProvider">
